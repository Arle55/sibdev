          a.page-header__upgrade-link(href="#")
            h1.page-header__upgrade-title Upgrade account
            p.page-header__upgrade-text Pay 10$, Yearly

      .page-header__user
        img.page-header__user-image(src="img/ava.png", alt="User avatar")
        .page-header__user-container
          a.page-header__user-link(href="#")
